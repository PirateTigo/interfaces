<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Accordion?>

<AnchorPane fx:id="contentRoot" xmlns:fx="http://javafx.com/fxml">
    <VBox AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0"
          AnchorPane.rightAnchor="0.0" AnchorPane.bottomAnchor="0.0">
        <HBox fx:id="helpContent">
            <VBox fx:id="helpMenuBox">
                <Accordion fx:id="helpMenu"/>
            </VBox>
            <VBox fx:id="webViewArea"/>
        </HBox>
    </VBox>
</AnchorPane>
